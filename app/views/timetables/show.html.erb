<p id="notice"><%= notice %></p>

<p>
  <strong>Gig name:</strong>
  <%= @timetable.gig.name %>
</p>
<p>
  <strong>Venue:</strong>
  <%= @timetable.venue.name %>
</p>

<p>
  <strong>Timetable date:</strong>
  <%= @timetable.try(:human_readable_date) %>
