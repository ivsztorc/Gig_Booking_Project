<h2>My Bookings</h2>
<table>
    <thead>
      <tr>
        <th>Gig</th>
        <th>Venue</th>
        <th>Timetable date</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @bookings.each do | booking | %>
      <tr>
        <td>
          <%= booking.timetable.gig.name %></td>
          <td><%= booking.timetable.venue.name %></td>

          <td><%= booking.timetable.human_readable_date %></td>
      </tr>

      <% end %>
    </tbody>


</table>

<br>