<p id="notice"><%= notice %></p>

<h1>Listing Timetables</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Timetable</th>
      <th>Timetable date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timetables.each do |timetable| %>
      <tr>
        <td><%= timetable.gig.name %></td>
        <td><%= timetable.timetable_id %></td>
        <td><%= timetable.timetable_date %></td>


        <!-- # might need to take it out -->
      <!-- <li> <%#= form_tag(timetables_path, method: "get") do %></li>
            <li><%#= text_field_tag(:name) %></li>
            <li><%#= submit_tag("Search Timetables") %></li>
            <li><%# end %></li> -->



        <td><%= link_to 'Show', timetable %></td>
        <td><%= link_to 'Edit', edit_timetable_path(timetable) %></td>
        <td><%= link_to 'Destroy', timetable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timetable', new_timetable_path %>